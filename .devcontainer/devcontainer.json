{
  "name": "fishing-web-app",
  "dockerComposeFile": [
    "../docker-compose.yml"
  ],
  "features": {
    "ghcr.io/devcontainers/features/docker-outside-of-docker:1": {}
  },
  "service": "dev",
  "workspaceFolder": "/workspace",
  "runServices": [
    "dev",
    "backend",
    "web",
    "db"
  ],
  "shutdownAction": "stopCompose",
  "forwardPorts": [
    8080,
    5173,
    5432
  ]
}
